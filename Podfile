workspace 'AutomaticKeyboard.xcworkspace'

## Frameworks targets
abstract_target 'Frameworks' do
	use_frameworks!
	target 'AutomaticKeyboard-iOS' do
		platform :ios, '8.0'
	end
end

## Tests targets
abstract_target 'Tests' do
	use_frameworks!
	target 'AutomaticKeyboardTests-iOS' do
		platform :ios, '8.0'
	end
end

## Samples targets
abstract_target 'Samples' do
	use_frameworks!
	target 'AutomaticKeyboardSample-iOS' do
		project 'Samples/AutomaticKeyboardSample-iOS/AutomaticKeyboardSample-iOS'
		platform :ios, '8.0'
	end
end
